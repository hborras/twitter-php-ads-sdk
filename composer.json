{
  "name": "hborras/twitter-php-ads-sdk",
  "type": "library",
  "description": "A Twitter supported and maintained Ads API SDK for PHP",
  "keywords": [
    "twitter",
    "api",
    "ads",
    "oauth",
    "rest",
    "social",
    "twitter api",
    "twitter oauth"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Hector Borras",
      "email": "hborrasaleixandre@gmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/hborras/twitter-php-ads-sdk",
    "issues": "https://github.com/hborras/twitter-php-ads-sdk/issues"
  },
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "phpstan/phpstan": "0.9.*"
  },
  "scripts": {
    "phpstan": "vendor/bin/phpstan analyse src --level=0"
  },
  "autoload": {
    "psr-4": {
      "Hborras\\TwitterAdsSDK\\": "src"
    }
  }
}
